{
    "manifest_version": 3,
    "name": "Word Definition Lookup",
    "version": "1.1",
    "description": "Shows definition of selected word with pronunciation.",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
    "https://api.dictionaryapi.dev/*",
    "*://*/*"
    ],
    "icons": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  }
  